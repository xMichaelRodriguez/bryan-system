{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "models": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::model.model",
      "mappedBy": "product"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "string",
      "required": true
    }
  }
}
